[WAT]

Test_Directory="res://tests"
Results_Directory="res://tests/results/WAT"
Minimize_Window_When_Running_Tests=false
TestStrategy={
"repeat": 1,
"strategy": "RunAll"
}
Tags=PoolStringArray(  )
Display=8

[application]

config/name="Pok√©mon Maker"
run/main_scene="res://Maps/map2/Map2.tscn"

[autoload]

Global="*res://Scenes/Global.tscn"

[display]

window/size/width=256
window/size/height=150
window/size/test_width=1024
window/size/test_height=600
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "WAT", "vnen.tiled_importer" )

